@model Spenders.ViewModels.GroupDetailsViewModel

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>All groups created</title>
</head>
<body>
    @foreach (var groupMember in Model.groupMembers)
    {
        <div>
            <h2>@groupMember.Group.Name</h2>
            <h4>@groupMember.Group.Name</h4>

            <h6>@groupMember.SpenderUser.Email</h6>
        </div>
    }
</body>
</html>
